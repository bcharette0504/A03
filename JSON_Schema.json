{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "patternProperties": {
    "^[0-9a-zA-Z]+$": {
      "type": "object",
      "properties": {
        "image": {
          "type": "string",
          "format": "uri"
        },
        "name": {
          "type": "string"
        }
      },
      "required": ["image", "name"],
      "additionalProperties": false
    }
  },
  "additionalProperties": false
}

